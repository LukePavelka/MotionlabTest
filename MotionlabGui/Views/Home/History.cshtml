@model IEnumerable<Motionlab.DB.InterviewTestDb>

@{
    ViewData["Title"] = "History";
}

<h1>History</h1>


    <div class="form-group">
        <form asp-action="History">
            <input type="submit" value="Remove All" name="submit" class="btn btn-primary" />
        </form>
    </div>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Key)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayName("Is Successful?")
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Error)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Key)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsSuccessful)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Error)
            </td>
        </tr>
}
    </tbody>
</table>
